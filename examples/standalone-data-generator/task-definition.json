{
  "family": "clickstream-sample-data-task-vir",
  "executionRoleArn": "arn:aws:iam::539689806100:role/clickstream-sample-data-ecs-task-role-new",
  "taskRoleArn": "arn:aws:iam::539689806100:role/clickstream-sample-data-ecs-task-role-new",
  "containerDefinitions": [
    {
      "name": "clickstream-sample-data",
      "image": "539689806100.dkr.ecr.ap-southeast-1.amazonaws.com/clickstream-sample-data-repo:vir",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/clickstream-sample-data-log-vir",
          "awslogs-region": "ap-southeast-1",
          "awslogs-stream-prefix": "ClickStream-Test"
        }
      }    
    }
  ], 
  "networkMode": "awsvpc",  
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048"
}
